%% Solution points

% Set write parameters
strVar = 'x, y';
varMat = [GLpoints(P), zeros(P+1,1)];
strFile = 'solPts_P2.dat';
strTitle = ['Variables = ', strVar];
dlmwrite(strFile, strTitle, 'delimiter', '');
dlmwrite(strFile, varMat, 'delimiter', '\t', '-append');

